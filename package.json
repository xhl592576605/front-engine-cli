{
  "name": "temp-cli",
  "version": "0.0.0",
  "description": "temp-cli",
  "main": "dist/temp-cli.cjs.js",
  "unpkg": "dist/temp-cli.global.js",
  "jsdelivr": "dist/temp-cli.global.js",
  "module": "dist/temp-cli.esm-bundler.js",
  "umd": "dist/temp-cli.umd.js",
  "types": "dist/temp-cli.d.ts",
  "outputName": "tempcli",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint \"src/**\" --fix  --ext .js，.jsx，.ts，.tsx --config .eslintrc.js ",
    "build": "npm run \"build:ts\" && npm run build:dts",
    "build:ts": "rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose && tail -n +5 src/globalExtensions.ts >> dist/temp-cli.d.ts ",
    "show": "yalc installations show",
    "watch": "nodemon --ignore dist/ --ignore node_modules/ --watch src/ -C -e ts,tsx,scss -x \"npm run build\""
  },
  "bin": {
    "temp-cli": "./bin/temp-cli.js"
  },
